<div class="outer"><div class="inner">
<p id="notice"><%= notice %></p>
<h1>詳細画面</h1>

<body style="background-color:#e0ffff;">
  <table initialTab="hyo">
   <tbody>
     <tr>
       <th>ID</th>
       <td><%= @user.id %></td>
     </tr>
     <tr>
       <th>名前</th>
       <td><%= @user.name %></td>
     </tr>
     <tr>
       <th>グループID</th>
       <td><%= @user.group_id %></td>
     </tr>
     <tr>
       <th>グループ名</th>
       <td><%= @user.group.gname %></td>
     </tr>
     <tr>
       <th>作成日時</th>
       <td><%= @user.created_at.to_s(:datetime_jp) %></td>
     </tr>
     <tr>
       <th>更新日時</th>
       <td><%= @user.updated_at.to_s(:datetime_jp) %></td>
     </tr>
    </tbody>
  </table>
</body>
</div></div>